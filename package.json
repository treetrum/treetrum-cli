{
    "name": "treetrum-cli",
    "version": "1.0.0",
    "description": "",
    "main": "index.js",
    "scripts": {
        "start": "ts-node src/index.ts",
        "prebuild": "rm -rf dist && rm -rf lib",
        "build": "tsc && pkg --target latest-macos-x64 dist/index.js -o ./lib/treetrum",
        "postbuild": "cp -R ./node_modules/puppeteer/.local-chromium/ lib/chromium",
        "prelink": "rm -rf /usr/local/bin/treetrum && rm -rf /usr/local/bin/chromium",
        "link": "ln -s $(pwd)/lib/treetrum /usr/local/bin/treetrum && ln -s $(pwd)/lib/chromium /usr/local/bin/chromium"
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "devDependencies": {
        "@types/dinero.js": "^1.6.5",
        "@types/node": "^14.14.22",
        "@types/node-fetch": "^2.5.8",
        "@types/pixelmatch": "^5.2.2",
        "@types/pngjs": "^3.4.2",
        "@types/prompts": "^2.0.9",
        "@types/xlsx": "^0.0.36",
        "pkg": "^4.4.9",
        "ts-node": "^9.1.1",
        "typescript": "^4.1.3"
    },
    "dependencies": {
        "axios": "^0.21.1",
        "chalk": "^4.1.0",
        "commander": "^7.0.0",
        "csv-parse": "^4.15.0",
        "csv-stringify": "^5.6.1",
        "date-fns": "^2.16.1",
        "dinero.js": "^1.8.1",
        "dotenv": "^8.2.0",
        "find-config": "^1.0.0",
        "g": "^2.0.1",
        "ing-au-login": "^2.0.3",
        "moment": "^2.29.1",
        "node-fetch": "^2.6.1",
        "pixelmatch": "^5.2.1",
        "pngjs": "^3.4.0",
        "prompts": "^2.4.0",
        "puppeteer": "^5.5.0",
        "qs": "^6.9.6",
        "query-string": "^6.13.8",
        "xlsx": "^0.16.9"
    }
}
